package intro;

public class recapDemo2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double[] myList = {1.2,3.5,5.3,2.1};
		double sub = 0;
		double max = 0;
		
		for(double i : myList) {
			System.out.println(i);
			sub+=i;
			if(max < i) {
				max = i;
			}
			
		}
		System.out.println(sub + "   " + max);
	}

}
