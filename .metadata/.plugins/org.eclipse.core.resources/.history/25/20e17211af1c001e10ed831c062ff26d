package oopIntro;

public class Product {
	
	public Product() {
		// TODO Auto-generated constructor stub
		System.out.println("Ben olu≈ütum");
	}
	
	public Product(int id , String name , double unitPrice , String detail){
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	int id;
	String name;
	double unitPrice;
	String detail;
	
	
}
